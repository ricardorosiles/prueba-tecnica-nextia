{
  "name": "prueba-eugenia",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development nodemon src/index.ts",
    "start:docker": "docker compose --env-file development.env up -d",
    "start:docker:test": "docker compose --env-file test.env up -d",
    "build": "rimraf dist && tsc",
    "lint": "eslint --ext .ts --ext .js src database scripts",
    "test": "jest --watchAll --collectCoverage=false",
    "test:integration": "jest -c jest.config.integration.js --testPathPattern=src/integrations --runInBand --silent",
    "test:integration:single": "jest -c jest.config.integration.js --runInBand --detectOpenHandles",
    "test:integration:single:watch": "jest -c jest.config.integration.js --watch --runInBand",
    "test:cov": "jest --no-cache --coverage",
    "prepare": "husky install",
    "migration:build": "rimraf migrations-dist && tsc --build database/tsconfig.json",
    "migration:script": "sh scripts/migration.sh",
    "migration:generate": "cross-env MIG_ENV=true npx sequelize-cli migration:generate --name",
    "migration:all": "yarn migration:build && npx sequelize-cli db:migrate",
    "migration:undo": "yarn migration:build && npx sequelize-cli db:migrate:undo --name",
    "migration:undo:all": "yarn migration:build && npx sequelize-cli db:migrate:undo:all",
    "migration:test:all": "yarn migration:build && cross-env NODE_ENV=test npx sequelize-cli db:migrate",
    "migration:test:undo": "yarn migration:build && cross-env NODE_ENV=test npx sequelize-cli db:migrate:undo --name",
    "migration:test:undo:all": "yarn migration:build && cross-env NODE_ENV=test npx sequelize-cli db:migrate:undo:all",
    "release": "release-it",
    "release:ci": "release-it --ci",
    "release:patch": "release-it -- patch",
    "release:patch:ci": "release-it -- patch --ci",
    "release:minor": "release-it -- minor --ci",
    "release:major": "release-it -- major --ci"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/bcrypt": "^5.0.2",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "fastest-validator": "^1.17.0",
    "firebase": "^10.7.1",
    "firebase-admin": "^12.0.0",
    "handlebars": "^4.7.8",
    "mailgun.js": "^9.4.1",
    "path": "^0.12.7",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "redis": "^4.6.12",
    "sequelize": "^6.35.2",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.4",
    "@commitlint/config-conventional": "^18.4.4",
    "@release-it/conventional-changelog": "^8.0.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.3",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.7",
    "cross-env": "^7.0.3",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": "20.10.0"
  }
}
